<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ban.teacherBAN">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ban.teacherBAN.ActivityAttendanceSheetToday"></activity>
        <activity android:name="com.ban.teacherBAN.ActivityCourseEnrollmentMode" />
        <activity android:name="com.ban.teacherBAN.ActivityAttendanceSheet" />
        <activity android:name="com.ban.teacherBAN.ActivityTeacherInfo" />
        <activity android:name="com.ban.teacherBAN.ActivityStartDeviceEnrollment" />
        <activity android:name="com.ban.teacherBAN.ActivityManualAttendance" />
        <activity android:name="com.ban.teacherBAN.ActivityDeviceAttendance" />
        <activity android:name="com.ban.teacherBAN.ActivityResource" />
        <activity android:name="com.ban.teacherBAN.ActivityMarks" />
        <activity android:name="com.ban.teacherBAN.ActivityQRcodeGenerator" />
        <activity android:name="com.ban.teacherBAN.ActivityAssignment" />
        <activity
            android:name="com.ban.teacherBAN.ActivityInsideCourse"
            android:label="@string/title_activity_course" />
        <activity android:name="com.ban.teacherBAN.ActivityCreateCourse" />
        <activity
            android:name="com.ban.teacherBAN.MainActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>